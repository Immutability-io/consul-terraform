{
  "service": {
    "name": "vault",
    "port": 8200,
    "tags": ["vault", "urlprefix-vault.service.consul/"],
    "check": {
      "id": "vault-health",
      "name": "Running on port 8200",
      "script": "VAULT_ADDR=vault_url vault status",
      "interval": "10s",
      "timeout": "1s"
    }
  }
}
